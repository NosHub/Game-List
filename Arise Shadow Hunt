-- Paragraph à¸ªà¸³à¸«à¸£à¸±à¸š log
local LogParagraph = Tabs.Randoms:AddParagraph({
    Title = "ðŸŽ² Gear Upgrade Log",
    Content = "â³ Waiting for start..."
})

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™ update log
local function updateLog(text)
    if LogParagraph and LogParagraph.SetDesc then
        LogParagraph:SetDesc(text)
    else
        LogParagraph.Content = text
    end
end

-- Toggle AutoGear
local ToggleAutoGear = Tabs.Randoms:AddToggle("AutoGearUpgrade", {
    Title = "Auto Gear Upgrade",
    Default = false
})

ToggleAutoGear:OnChanged(function(state)
    task.spawn(function()
        while ToggleAutoGear.Value do
            task.wait()
            pcall(function()
                -- à¹€à¸Šà¹‡à¸„à¸à¸²à¸£à¹€à¸¥à¸·à¸­à¸ Gear à¹à¸¥à¸° Grades
                if not selectedGearGUID then
                    updateLog("âŒ à¸¢à¸±à¸‡à¹„à¸¡à¹ˆà¹„à¸”à¹‰à¹€à¸¥à¸·à¸­à¸ Gear")
                    ToggleAutoGear:SetValue(false)
                    return
                end
                if #SelectedGrades == 0 then
                    updateLog("âŒ à¸¢à¸±à¸‡à¹„à¸¡à¹ˆà¹„à¸”à¹‰à¹€à¸¥à¸·à¸­à¸ Target Grades")
                    ToggleAutoGear:SetValue(false)
                    return
                end

                -- detect drawId à¸•à¸²à¸¡à¸•à¸±à¸§à¸­à¸±à¸à¸©à¸£à¸™à¸³à¸«à¸™à¹‰à¸²à¹„à¸­à¹€à¸—à¹‡à¸¡
                local firstChar = string.sub(selectedGearName,1,1)
                local drawId
                if firstChar == "R" then
                    drawId = 980001
                elseif firstChar == "Y" then
                    drawId = 980002
                elseif firstChar == "B" then
                    drawId = 980003
                end

                -- à¹€à¸£à¸µà¸¢à¸ Remote à¸”à¹‰à¸§à¸¢ format à¹ƒà¸«à¸¡à¹ˆ
                local args = {
                    {
                        drawId = drawId,
                        guid = selectedGearGUID
                    }
                }

                local success, result = pcall(function()
                    return Remotes.RandomHeroEquipGrade:InvokeServer(unpack(args))
                end)

                if not success or not result then
                    updateLog("âŒ Remote Failed")
                    return
                end

                -- get gradeId à¸ˆà¸²à¸à¸œà¸¥à¸¥à¸±à¸žà¸˜à¹Œ
                local gradeIdGot = result.data and tonumber(result.data.grade)
                local gradeTitleGot = "Unknown"

                -- à¹à¸›à¸¥à¸‡ gradeId â†’ à¸•à¸±à¸§à¸­à¸±à¸à¸©à¸£ à¹‚à¸”à¸¢à¹€à¸Šà¹‡à¸„à¸—à¸±à¹‰à¸‡ R/Y/B
                for _, g in ipairs(GradeIds) do
                    if tonumber(g.RId) == gradeIdGot or tonumber(g.YId) == gradeIdGot or tonumber(g.BId) == gradeIdGot then
                        gradeTitleGot = g.Title
                        break
                    end
                end

                -- update log à¹€à¸‰à¸žà¸²à¸°à¸•à¸±à¸§à¸­à¸±à¸à¸©à¸£
                local logText = string.format("à¸ªà¸¸à¹ˆà¸¡ %s â†’ %s", selectedGearName, gradeTitleGot)
                updateLog(logText)

                -- check target grade
                for _, target in ipairs(SelectedGrades) do
                    if gradeTitleGot == target then
                        updateLog(string.format("ðŸŽ¯ à¹„à¸”à¹‰à¹€à¸à¸£à¸”à¹€à¸›à¹‰à¸²à¸«à¸¡à¸²à¸¢! â†’ %s\nðŸ›‘ à¸«à¸¢à¸¸à¸” AutoGearUpgrade", gradeTitleGot))
                        ToggleAutoGear:SetValue(false)
                        return
                    end
                end

            end)
        end
    end)
end)

